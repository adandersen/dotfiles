### directory shortcuts
alias dev="cd c:/dev/code"
alias all="cd ~/dev/code/java-all-projects"
alias directory="cd ~/dev/code/directory-service"
alias sme="cd ~/dev/code/sme-service"
alias api="cd ~/dev/code/api-service"
alias form="cd ~/dev/code/form-service"
alias vx="cd ~/dev/code/vertx-shared"
alias pdf="cd ~/dev/code/pdf-service"
alias eshared="cd ~/dev/code/efile-shared"
alias irssim="cd ~/dev/code/irs-simulator"
alias efile="cd ~/dev/code/efile-service"
alias dotfiles="cd ~/.dotfiles"
alias crm="cd ~/dev/code/crm-service"
alias invoice="cd ~/dev/code/invoice-service"
alias acc="cd ~/dev/code/accounting-service"

alias cal='gcalcli --calendar alan.andersen@canopytax.com calw 1'
alias activate='. ./.venv/bin/activate'

alias alembicup="virtenv && alembic upgrade head && deactivate"
alias alembicdown="virtenv && alembic downgrade -1 && deactivate"
alias virtenv="pwd > .venv/lib/python3.7/site-packages/root.pth && source .venv/bin/activate"
